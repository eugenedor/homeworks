@model ToolsStore.WebUI.Models.ProductsListViewModel

@{
    ViewBag.Title = @ViewBag.NameApp;
}

@foreach (var p in Model.Products)
{
    Html.RenderPartial("ProductSummary", p);
}

<div class="pager">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List",
        new { page = x, equipment = Model.CurrentEquipment, category = Model.CurrentCategory }))
</div>